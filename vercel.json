{
  "devCommand": "uvicorn src.api.server:app --host 0.0.0.0 --port 3001",
  "buildCommand": "pip install uv && uv venv && source .venv/bin/activate && uv pip install -r uv.lock",
  "builds": [
    {
      "src": "src/api/server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/server.py"
    }
  ]
}
